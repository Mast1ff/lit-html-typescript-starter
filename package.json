{
    "name": "lpsample",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "webpack:dev": "cross-env NODE_ENV=development npx webpack --config ./webpack.config.js -w",
        "webpack:build": "cross-env NODE_ENV=production npx webpack --config ./webpack.config.js",
        "scss:dev": "cross-env NODE_ENV=development npx gulp sass:watch",
        "scss:build": "cross-env NODE_ENV=production npx gulp sass",
        "build:dev": "concurrently --kill-others \"yarn webpack:dev\" \"yarn scss:dev\"",
        "serve": "nodemon server.js --watch src/**/* -e ts,scss",
        "dev": "concurrently --kill-others \"yarn webpack:dev\" \"yarn scss:dev\" \"yarn serve\"",
        "build": "yarn webpack:build && yarn scss:build"
    },
    "dependencies": {
        "express": "^4.17.1",
        "hyphenate-style-name": "^1.0.4",
        "intersection-observer": "^0.11.0",
        "lit-html": "^1.3.0",
        "matchmediaquery": "^0.3.1",
        "swiper": "^6.3.5"
    },
    "devDependencies": {
        "@types/swiper": "^5.4.1",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "autoprefixer": "^10.0.1",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.13.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "gulp": "^4.0.2",
        "gulp-postcss": "^9.0.0",
        "gulp-sass": "^4.1.0",
        "html-webpack-plugin": "^4.5.0",
        "nodemon": "^2.0.6",
        "postcss": "^8.1.6",
        "ts-loader": "^8.0.11",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.0.5",
        "typescript-lit-html-plugin": "^0.9.0",
        "webpack": "^5.4.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    }
}
